  "compilationOptions": {
    "keyFile": "../../tools/Key.snk"
  },
  "authors": [ "Microsoft" ],
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/nodeservices"
  },
  "dependencies": {
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0-*",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0-*",
