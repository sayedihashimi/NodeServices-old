    <!--<Message Text="&#xD;&#xA;OutputPath: $(outputpath)&#xD;&#xA;pwScriptPath: $(pwScriptPath)&#xD;&#xA;pwTemplateFilePath: $(pwTemplateFilePath)&#xD;&#xA;pwVsixFilePath: $(pwVsixFilePath)&#xD;&#xA;pwScript: @(pwScript->'%(FullPath)')" Importance="high" />-->
